{
    "name": "student/wordpress-install-template",
    "description": "template d'install wordpress",
    "extra": {
        "wordpress-install-dir": "wp"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require": {
        "johnpbloch/wordpress": "^5.5",
        "wpackagist-theme/twentytwenty": "^1.5"
    }
}
